/*
math 함수를 넣었어야함 

int pyung( int a, int b)
{
	return pow(a, b);
}
int main()
{
	int x, y;

	scanf(" %d %d", &x, &y);

	printf("%d", pyung(x, y));

	return 0;

}

int sna(int x, int y, char z)
{
	switch (z)
	{
	case '+':
		return x + y;
	case '-':
		return x - y;
	case '/':
		return x / y;
	case '*':
		return x * y;
	}
	return 0;
}
int main() {
	int a, b;
	char c;

	scanf("%d %c %d", &a, &c, &b);
	printf("%d %c %d = %d\n", a, c, b, sna(a, b, c));
	return 0;
} */


/*
c++에서 해야되는 것 같은데 
void swwap(int x, int y) {
	int imsi;

	imsi = x;
	x = y;
	y = imsi;

	printf("첫 번째 함수 실행중 x=%d,y=%d \n", x, y);

}

void swwapfe (int &x, int &y) {
	int imsi;

	imsi = x;
	x = y;
	y = imsi;

	printf("두번째 함수 실행중x=%d,y=%d \n", x, y);
}
int main()
{
	int a, b;
	printf("두 수를 입력하세요.");
	scnaf("%d %d ", &a, &b);

	swwap(a, b);
	printf("첫 번째 함수 실행 후 a= %d,b= %d \n", a, b);

	swwapfe(a, b);
	printf("첫 번째 함수 실행 후 a= %d,b= %d \n", a, b);

	return 0;
	
}
