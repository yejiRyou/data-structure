int main() {
	int i, j;
	int pas[6][6] = {0};

	pas[0][1] = 1;
	for (i = 1; i < 6; i++) {
		for (j = 1; j < 6; j++) {
			pas[i][j] = pas[i-1][j] + pas[i ][j-1];
		}
	}
	for (i = 1; i < 6; i++) {
		for (j = 1; j < 6; j++) {
			printf(" %d", pas[i][j]);
		}
		printf("\n");
	}
	return 0;
}
